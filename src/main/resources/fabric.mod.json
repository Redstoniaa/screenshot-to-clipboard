{
  "schemaVersion": 1,
  "id": "screenshot-to-clipboard",
  "version": "${version}",

  "name": "Screenshot to Clipboard",
  "description": "Quick Minecraft mod that saves screenshots you take to the clipboard, though probably somewhat inefficiently.",
  "authors": [
  ],
  "contact": {
    "homepage": "https://github.com/Redstoniaa/screenshot-to-clipboard",
    "sources": "https://github.com/Redstoniaa/screenshot-to-clipboard"
  },

  "license": "MIT",
  "icon": "assets/screenshot_to_clipboard/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "screenshot_to_clipboard.ScreenshotToClipboardClient"
    ]
  },
  "mixins": [
    "screenshot_to_clipboard.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.19",
    "fabric-api": "*",
    "minecraft": "~1.19.4",
    "java": ">=17"
  }
}
