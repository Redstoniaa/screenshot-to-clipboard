{
  "schemaVersion": 1,
  "id": "screenshot-to-clipboard",
  "version": "${version}",

  "name": "Screenshot to Clipboard",
  "description": "Quick Minecraft mod that saves screenshots you take to the clipboard.",
  "authors": [
  ],
  "contact": {
    "homepage": "https://github.com/Redstoniaa/screenshot-to-clipboard",
    "sources": "https://github.com/Redstoniaa/screenshot-to-clipboard"
  },

  "license": "MIT",
  "icon": "assets/screenshot_to_clipboard/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "screenshot_to_clipboard.ScreenshotToClipboardMod::onInitialize"
    ]
  },
  "mixins": [
    "screenshot_to_clipboard.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.19",
    "minecraft": [ "1.18.x", "1.19.x", "1.20.x" ],
    "java": ">=17"
  }
}
